<div class="col-sm-12">
<div class="col-md-6 col-md-offset-3">
  <h1>Celkové výsledky</h1>
    <table class="table table-striped table-hover">
    <% @sum_all_points.each_with_index do |r,index| %>
    <tr>
      <td><%= index + 1 %>.</td>
      <td><%= @users.find(r[0]).fullname %></td>
      <td><%= r[1] %></td>
    </tr>
    <% end %>
  </table>
</div>




  <% @results.each do |result| %>
<div class="col-md-6">
  <h1><%= result[0].name %></h1>
    <table class="table table-striped table-hover">
    <% result[1].each_with_index do |r,index| %>
    <tr>
      <td><%= index + 1 %>.</td>
      <td><%= @users.find(r[0]).fullname %></td>
      <td><%= r[1] %></td>
    </tr>
    <% end %>
  </table>
  </div>
  <% end %>

</div>