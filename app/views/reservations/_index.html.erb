
<div id="week">

  <table class='table table-striped table-hover'>
    <thead>
      <tr>
        <th>Den</th>
        <th>Datum</th>
        <th>Čas</th>
        <th>Místo</th>
        <th>Kapacita</th>
        <th>Typ</th>
        <th colspan="4"></th>
      </tr>
    </thead>

    <% @reservations.group_by(&:week).each do |week, reservations| %>

    <tbody>
      <%= render partial: 'reservation', collection: reservations, :as => :reservation %>
    </tbody>
    <% end %>
  </table>
</div>











