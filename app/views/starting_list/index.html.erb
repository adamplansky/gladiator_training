<h1>Startovni listina</h1>
<table class="table table-striped table-hover">
  <tr>
    <th class="text-center">Číslo</th>
    <th class="text-center">Jméno</th>
    <th class="text-center">Tým</th>
    <th class="text-center">Zaplaceno</th>
  </tr>
  <% @gt_registrations.each do |reg| %>
  <tr>
    <td class="text-center"><%= reg.id %></td>
    <td class="text-center"><%= reg.firstname %> <%= reg.surname %></td>
    <td class="text-center"><%= reg.team %></td>
    <td class="text-center <%= zaplaceno_cls(reg.payed) %> w30"><%= zaplaceno(reg.payed)%></td>
  </tr>

  <% end %>
</table>
