<h1>Startovni listina</h1>
<table class="table table-striped table-hover">
  <tr>
    <th>Číslo</th>
    <th>Jméno</th>
    <th>Tým</th>
  </tr>
  <% @gt_registrations.each do |reg| %>
  <tr>
    <td><%= reg.id %></td>
    <td><%= reg.firstname %> <%= reg.surname %></td>
    <td><%= reg.team %></td>
  </tr>

  <% end %>
</table>
