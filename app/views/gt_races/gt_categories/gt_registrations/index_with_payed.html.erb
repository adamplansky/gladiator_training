<div class="container">
  <div class='col-sm-12'>
    <p id="notice"><%= notice %></p>

    <h1>Listing Gt Registrations</h1>

    <table class="table table-striped table-hover">
      <thead>
        <tr>
          <th>VS</th>
          <th>Jm√©no</th>
          <th>Team</th>
          <th>Gt race</th>
          <th>Price</th>
          <th>Sex</th>
          <th>Notes</th>
          <th>Payed</th>
          <th colspan="2"></th>
        </tr>
      </thead>

      <tbody>
        <% @grouped_by_gt_category.each do |gt_registrations| %>
        <tr>
          <td colspan="10" class='tdheader-gtreg'>
            <strong>
              <%= @categories.find(gt_registrations[0]).name %>
            </strong>
          </td>
        </tr>
        <% gt_registrations[1].each do |gt_registration| %>
        <tr>
          <%= render "index_with_payed", :gt_registration => gt_registration %>
        </tr>
        <% end %>

        <% end %>

      </tbody>
    </table>
    <br>
  </div>
</div>
