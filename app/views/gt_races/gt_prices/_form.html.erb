<%= form_for([@gt_race, @gt_price]) do |f| %>
  <% if @gt_price.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@gt_price.errors.count, "error") %> prohibited this gt_price from being saved:</h2>

      <ul>
      <% @gt_price.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label "Do toho data:" %><br>
    <%= f.date_select :until %>
  </div>
