    <%= f.label :gt_race_id %><br>
    <%= f.text_field :gt_race_id, value: @gt_race.id, readonly: true %>
  </div>
  <div class="field">
    <%= f.label "kategory id" %><br>
    <%= f.collection_select :gt_category_id, GtCategory.all, :id, :name %>
  </div>

  <div class="field">
    <%= f.label "Cena" %><br>
    <%= f.text_field :price %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
