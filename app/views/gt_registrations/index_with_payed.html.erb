<div class="container">
  <div class='col-sm-12'>
    <p id="notice"><%= notice %></p>

    <h1>Listing Gt Registrations</h1>

    <table class="table table-striped table-hover">
      <thead>
        <tr>
          <th>VS</th>
          <th>Jm√©no</th>
          <th>Team</th>
          <th>Gt race</th>
          <th>Price</th>
          <th>Sex</th>
          <th>Notes</th>
          <th>Payed</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @gt_registrations.each do |gt_registration| %>
        <tr>
          <td><%= gt_registration.id %></td>
          <td><%= gt_registration.firstname %> <%= gt_registration.surname %></td>
          <td><%= gt_registration.gt_race.team %></td>
          <td><%= gt_registration.gt_race.name %></td>
          <td><%= gt_registration.price %></td>
          <td><%= gt_registration.sex %></td>
          <td><%= gt_registration.notes %></td>
          <td><%= zaplaceno(gt_registration.payed) %></td>
          <td><%= link_to 'Show', gt_registration %></td>
          <td><%= link_to 'Edit', edit_gt_registration_path(gt_registration) %></td>
          <td><%= link_to 'Destroy', gt_registration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
    <br>
    <%= link_to 'New Gt registration', new_gt_registration_path %>
  </div>
</div>
